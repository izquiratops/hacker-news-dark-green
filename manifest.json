{
    "manifest_version": 2,
    "name": "Hacker News Dark mode",
    "version": "1.0",
    "description": "Hacker News dark theme",
    "content_scripts": [
      {
        "matches": ["https://news.ycombinator.com/*"],
        "css": ["styles.css"],
        "js": ["content-script.js"]
      }
    ],
    "permissions": [
      "https://news.ycombinator.com/*"
    ]
  }